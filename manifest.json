{
    "name": "Annoying pannel remover",
    "action": {
        "default_title": "Click to edit the configuration"
    },
    "options_ui": {
      "page": "options.html",
      "open_in_tab": true
    },
    "manifest_version": 3,
    "version": "1.0",
    "description": "Remove annoying parts from some html websites",
    "permissions": [
        "activeTab",
        "tabs",
        "scripting",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
      },
    "host_permissions": [
        "*://*/*"
    ],
    "icons": {
      "16": "icons/cleaner16.png",
      "48": "icons/cleaner48.png",
     "128": "icons/cleaner128.png"
    }
  }